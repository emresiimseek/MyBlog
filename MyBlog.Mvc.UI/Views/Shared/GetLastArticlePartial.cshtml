@model List<Article>

@using MyBlog.EntityFramework.Concrete
<div class="col-lg-12">
    <div class="sidebar-item recent-posts">
        <div class="sidebar-heading">
            <h2>Son Yazılar</h2>
        </div>
        <div class="content">
            <ul>
                @foreach (Article article in Model)
                {
                    <li>
                        <a href="post-details.html">
                            <h5>@article.Title</h5>
                            <span>@article.CreatedOn</span>
                        </a>
                    </li>
                }
            </ul>
        </div>
    </div>
</div>