@model List<Category>
<div class="col-lg-12">
    <div class="sidebar-item categories">
        <div class="sidebar-heading">
            <h2>Kategoriler</h2>
        </div>
        <div class="content">

            @foreach (Category category in (List<Category>)Session["cat"])
            {
                <ul>
                    <li>@Html.ActionLink(category.Title, "getArticlesByCategory", "Articles", new { id = category.Id }, new { @class = "dropdown-item nav_brand" })</li>

                </ul>
            }

        </div>
    </div>
</div>
